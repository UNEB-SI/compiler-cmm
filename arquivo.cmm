/* Sequencia de Fibonacci em Cmm - Recursivo */

prototipo inteiro fibonacci(inteiro);
prototipo inteiro contador(inteiro);

inteiro fibonacci(inteiro numer) {
     inteiro x, n, y;
     x = 1;
     n = x;
     numer = 3 * 3;
		 retorne numer;
}

inteiro contador(inteiro numero){
	inteiro x, y;
	para(x = 0; x < numero; x = x+1){
		y = x;
	}
	retorne y;
}

